<script type="module">
import dc from "/js/directPageCall.js";
import jsl from "/js/jsl.js";
import html2 from "/js/html2.js";
import { setConfig } from "/js/config.js";
setConfig({"ZIGBEE":false,"GUI_IS_POWERLINE":false,"isDebug":false,"gu_type":"release","WLAN":{"is_double_wlan":false},"GUI_IS_REPEATER":false,"GUI_IS_GATEWAY":false,"language":"de","GUI_HIDE_TEASER":false});
function block(blocktime) {
var form = jsl.get("uiMainForm");
if (form) {
if (blocktime > 0) {
doBlockTicket(blocktime * 1000);
}
}
}
function getWaitStr(mSec) {
var sec = Math.ceil(mSec / 1000),
gTxtWaitmore = "Bitte warten Sie %1 Sekunden.",
gTxtWaitone = "Bitte warten Sie 1 Sekunde.";
if (sec === 1) {
return gTxtWaitone;
} else {
return gTxtWaitmore.replace(/%1/, sec);
}
}
function doBlockTicket(time) {
var start = (new Date()).getTime();
var timer = timer || null;
jsl.disable("uiTicket");
jsl.disable("uiSendTicket");
if (timer) {
window.clearTimeout(timer);
timer = null;
}
function f() {
var now = (new Date()).getTime();
var wait = time - (now - start);
if (wait > 0) {
jsl.setHtml("uiWait", getWaitStr(wait));
jsl.show("uiLoginError");
timer = window.setTimeout(f, 500);
} else {
jsl.hide("uiLoginError");
jsl.enable("uiTicket");
jsl.enable("uiSendTicket");
}
}
f();
}
/**
* Erstellt Hinweis das der Internetzugang gesperrt ist
*/
function getAccessBlocked() {
return html2.div({class: "space"},
html2.div({}, "Gerätesperre aktiv: Der Internetzugang Ihres Gerätes ist gesperrt.")
);
}
/**
* gibt Button zurück für Weiterleitung zum Log-In der Fritz!Box
*/
function getBackButton() {
// eslint-disable-next-line standard/object-curly-even-spacing
return html2.fragment(
html2.hr(),
html2.div({class: "button_tobox"},
html2.button({type: "button", id: "uiTobox", onclick: function () { dc.call("/start") }, name: "tobox" }, "Zur FRITZ!Box")
));
}
window.onload = function () {
var isAllowed = true;
var blockTime = 0;
if (!isAllowed) {
var page = jsl.get("page_content");
if (page) {
html2.add(page, getAccessBlocked());
//html2.add(page, getBackButton());
}
}
block(blockTime);
}
</script>
